<input type='button' onClick='check()' value='Submit' />
<div id="output">validating.....</div>
<script>
function check() {
	var r='',
	var id= getValue('id'),
	var password= getValue('pass')
	for(obj of U){
		if(obj.id==id && obj.pass==password)
			r+='success'
			break;
		else
			r+='failure'
	}
	document.getElementById('output').innerHTML=r
}




</script>

