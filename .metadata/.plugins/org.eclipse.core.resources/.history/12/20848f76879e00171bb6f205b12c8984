function script2() {
	var s='';
	 s+= '<h3>Questions</h3><ol>'
	for( var i = 0; i < Q.length; ++i){
    	s+='<li>'+Q[i].statement+'</li><br>';
    	
    	for( var j=0;j<4;++j){
    		
    		s+='<input type="radio" name="q" value="a" id="q1a"><label for="q1a">'+Q[i].choices[j]'+</label><br/>
    		s+='Option '+j+') '+Q[i].choices[j]+'<br><br>';
    	}
    	
        
        }
	 s+='</ol>'
	 document.getElementById('ExamQuestions').innerHTML = s;
}
	
	
	
	




